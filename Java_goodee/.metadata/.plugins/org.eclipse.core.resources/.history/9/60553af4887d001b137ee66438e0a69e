package ThisJavaThread;

import java.awt.Toolkit;

public class BeepTask implements Runnable {

	public void run() {
		
		// 여기부터
		Toolkit toolkit = Toolkit.getDefaultToolkit();
		for(int i=0; i<5; i++) {
			toolkit.beep();
			try {BeepPrintEx1.sleep(500); } catch(Exception e) {}
			
		}// 여기까지 스레드 실행내용 
		
	}
	
	
}

