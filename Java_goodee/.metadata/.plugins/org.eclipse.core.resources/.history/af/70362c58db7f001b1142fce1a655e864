package Java_Nam;

import java.util.Random;
import java.util.stream.IntStream;

public class StreamEx2 {

	public static void main(String[] args) {

		// Random클래스 객체 생성 .ints 메소드 호출
		// int난수 스트림 반환
		IntStream intStream = new Random().ints();
		
		// 스트림 출력
		// limit(5) : 5까지로 자르고
		intStream.limit(5).
	}

}
