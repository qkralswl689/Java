package ThisJavaThread;

import java.awt.Toolkit;

public class Thread {
	
	public static void main(String[] args) {
		
		// Toolkit 객체 생성
		Toolkit toolkit = Toolkit.getDefaultToolkit();
		for(int i=0; i<5; i++) {
			// 비프음 발생
			toolkit.beep(); 
			try { Thread.sleep(500);} catch(Exception e) {}
		}
		}
	}
}

